{
   "name": "404-starter",
   "private": true,
   "scripts": {
      "build": "turbo run build",
      "dev": "turbo run dev",
      "lint": "turbo run lint",
      "format": "prettier --write \"**/*.{ts,tsx,md}\"",
      "check-types": "turbo run check-types",
      "api:typegen": "cd apps/api && npm run cf-typegen"
   },
   "devDependencies": {
      "prettier": "^3.6.2",
      "turbo": "^2.5.4",
      "typescript": "5.8.3"
   },
   "engines": {
      "node": ">=18"
   },
   "packageManager": "bun@1.2.15",
   "workspaces": [
      "apps/*",
      "packages/*"
   ]
}

